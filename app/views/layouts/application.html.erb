<!DOCTYPE html>
<html>
<head>
  <title>DigiCivic</title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body id="bodycss">


<div id="fb-root"></div>
      <script>
        window.fbAsyncInit = function() {
              FB.init({
            appId      : '205528859544975',
            channelUrl : '//elevote50.herokuapp.com/channel.html',
            status     : true,
            cookie     : true,
            xfbml      : true,
            oauth      : true,
          });
        };
        (function(d){
           var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
           js = d.createElement('script'); js.id = id; js.async = true;
           js.src = "//connect.facebook.net/en_US/all.js";
           d.getElementsByTagName('head')[0].appendChild(js);
         }(document));
      </script>



<div class="container">






  <div id="leftColumn" class="span-5">





      <%= yield :leftColumn %>
      </div>


      <div id="middleColumn" class="span-14">
            <%= yield :middleColumn%>
      </div>

      <div id="rightColumn" class="span-5 last">
            <%= yield :rightColumn %>
      </div>


<div id="footer" class="span-24">

  Footer  Copyright Digital Politics, LLC.  2012.
</div>


</div> <!--  container -->



</body>
</html>
