<% content_for :leftColumn do %>

<% end %>

<% content_for :middleColumn do %>

      <%= form_for @official, :html => {:multipart => true } do |f| %>



        <%= f.label :email %>
        <%= f.text_field :email %>
       <br>
        <%= f.label :portrait %>
        <%= f.file_field :portrait %>
        <br>
         <%= f.label "About Me" %>
        <%= f.text_area :aboutMe %>
        <br>
          <%= f.label "Positions" %>
        <%= f.text_area :positions %>
        <br>
        <%= f.label "Achievements" %>
        <%= f.text_area :achievements %>
        <br>
        <%= f.label "Goals" %>
        <%= f.text_area :goals %>
        <br>
        <%= f.label "Campaign Slogan" %>
        <%= f.text_field :campaignSlogan %>
        <br>
        <%= f.label "Office" %>
        <%= f.text_field :office %>
        <br>
        <%= f.submit %>



    <% end %>

<% end %>