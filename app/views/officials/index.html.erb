
<% content_for :middleColumn do %>



    <h1>Facebook Client-side Authentication Example</h1>

      <div id="auth-loggedout">
        <a href="#" id="auth-loginlink">Login</a>
      </div>

    
    
    <%= request.env['omniauth.auth'] %>


<h1>Your Elected Officials</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Office</th>

    <th>Slogan</th>
    <th></th>
  </tr>

<% @officials.each do |official| %>
  <tr>

    <td><%= link_to officials_full_name(official), official %></td>
    <td><% if official.office %><%= official.office %><% end %></td>
    <td><%= official.campaignSlogan %></td>
  </tr>
<% end %>
</table>

<br />

<% end %>

<% content_for :rightColumn do %>



<% end %>